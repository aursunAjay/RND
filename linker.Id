ENTRY(reset_handler)

MEMORY
{
    CCMRAM    (xrw)    : ORIGIN = 0x10000000,   LENGTH = 64K
    RAM       (xrw)    : ORIGIN = 0x20000000,   LENGTH = 128K
    FLASH     (rx)     : ORIGIN = 0x8000000,    LENGTH = 1024K
}

SECTION
{
    
    .text:
    {
        . = ALIGN(4);
        *(.isr_vector)   
        *(.text)
        *(.rodata)
    }FLASH  
 
    .data:
    {
        . = ALIGN(4);
        *(.data)
    }RAM AT> FLASH  
    
    .bss:
    {
        . = ALIGN(4);
        *(.bss)
    }RAM 

}